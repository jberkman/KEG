@LAZYGLOBAL OFF.
DECLARE PARAMETER LIB.
DECLARE LOCAL CACHE IS LEX().
PRINT"$ USE.KS".
LIB:ADD({
    DECLARE PARAMETER P.
    IF NOT CACHE:HASKEY(P){
        //IF NOT EXISTS(P)COPYPATH("0:"+P,P).
        DECLARE LOCAL LIB IS LIST().
        PRINT(". "+P).
        RUNPATH("0:"+P,LIB).
        SET CACHE[P]TO LIB[0].
    }
    RETURN CACHE[P].
}).
